<!-- Content Header (Page header) -->
<section class="content-header">
  <h1 style="visibility: hidden">Registrate</h1>
  <% if soy_fundacion %>
      <% provide(:title, 'Alta Alumno Becado') %>
  <% else
       if noIniciaronSesion or noEsUsuarioAdministrador %>
          <% provide(:title, 'Registrarse') %>
      <% else
           if esUsuarioAdministrador %>
              <% provide(:title, 'Crear Nueva Fundación') %>
          <% end %>
      <% end %>
  <% end %>
  <ol class="breadcrumb">
    <li><a href="/"><i class="fa fa-home"></i>Inicio
    </a></li>
    <li class="active">Nuevo usuario</li>
  </ol>
</section>
<section class="content">
  <div class="box box-primary col-md-offset-2 col-md-8">
    <div class="box-body box-profile">
      <% if soy_fundacion %>
          <%= render :partial => 'tab_form_user', :locals => {:view => 'create_becado', :btn_label => 'Registrar'} %>
      <% else
           if noIniciaronSesion or noEsUsuarioAdministrador %>
              <%= render :partial => 'tab_form_user', :locals => {:view => 'create', :btn_label => 'Registrarme'} %>
          <% else
               if esUsuarioAdministrador %>
                  <%= render :partial => 'tab_form_fundacion', :locals => {:view => 'create', :btn_label => 'Alta Fundación'} %>
              <% end %>
          <% end %>
      <% end %>
    </div>
  </div>
</section>